g_pkg_flg = -bs-package-name tea
src_root_dir = /home/pierre/dev/imago_front
bsc = /home/pierre/dev/imago_front/node_modules/bs-platform/lib/bsc.exe
bsdep = /home/pierre/dev/imago_front/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101 -warn-error +101
bsc_flags = -bs-suffix -nostdlib -I '/home/pierre/dev/imago_front/node_modules/bs-platform/lib/ocaml' -bs-super-errors -bs-g -color always
ppx_flags = 
g_pkg_incls = -I /home/pierre/dev/imago_front/node_modules/bucklescript-tea/lib/ocaml -I /home/pierre/dev/imago_front/node_modules/tablecloth-bucklescript/lib/ocaml
bs_package_dev_includes = 
g_ns = 
bsb_dir_group = 0
g_lib_incls = -I src
rule build_ast_and_module_sets
  command = $bsc  $pp_flags $ppx_flags $warnings $bsc_flags -c -o $out -bs-syntax-only -bs-binary-ast $in
  description = [34mBuilding[39m [2m${out}[22m
build src/app.mlast : build_ast_and_module_sets $src_root_dir/src/app.ml
rule build_deps
  command = $bsdep $g_ns -g $bsb_dir_group $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/app.d : build_deps src/app.mlast
rule ml_cmj_cmi
  command = $bsc $g_pkg_flg $g_pkg_incls $g_lib_incls $bsc_extra_includes $warnings $bsc_flags $gentypeconfig -o $out -c  $in $postbuild
  dyndep = $in_e.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/app.cmj |  $src_root_dir/lib/es6/src/app.bs.js src/app.cmi : ml_cmj_cmi src/app.mlast ||  src/app.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:lib/es6/src
build src/chat.mlast : build_ast_and_module_sets $src_root_dir/src/chat.ml
build src/chat.d : build_deps src/chat.mlast
build src/chat.cmj |  $src_root_dir/lib/es6/src/chat.bs.js src/chat.cmi : ml_cmj_cmi src/chat.mlast ||  src/chat.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:lib/es6/src
build src/main.mlast : build_ast_and_module_sets $src_root_dir/src/main.ml
build src/main.d : build_deps src/main.mlast
build src/main.cmj |  $src_root_dir/lib/es6/src/main.bs.js src/main.cmi : ml_cmj_cmi src/main.mlast ||  src/main.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:lib/es6/src
build src/matrix.mlast : build_ast_and_module_sets $src_root_dir/src/matrix.ml
build src/matrix.d : build_deps src/matrix.mlast
build src/matrix.cmj |  $src_root_dir/lib/es6/src/matrix.bs.js src/matrix.cmi : ml_cmj_cmi src/matrix.mlast ||  src/matrix.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:lib/es6/src
build src/router.mlast : build_ast_and_module_sets $src_root_dir/src/router.ml
build src/router.d : build_deps src/router.mlast
build src/router.cmj |  $src_root_dir/lib/es6/src/router.bs.js src/router.cmi : ml_cmj_cmi src/router.mlast ||  src/router.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:lib/es6/src
