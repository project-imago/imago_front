{
  "name": "imago_front",
  "version": "0.1.0",
  "scripts": {
    "bsb-clean": "bsb -clean-world",
    "bsb-build": "bsb -make-world",
    "bsb-watch": "bsb -make-world -w -ws _",
    "webpack-server": "webpack-dev-server",
    "webpack-server-tls": "webpack-dev-server --https --public imago-dev.img --key imago-dev.img.tls.key --cert imago-dev.img.tls.crt",
    "server": "concurrently -k \"npm:bsb-watch\" \"npm:webpack-server\"",
    "server-tls": "concurrently -k \"npm:bsb-watch\" \"npm:webpack-server-tls\""
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "^6.0.3",
    "concurrently": "^4.1.2",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "bucklescript-tea": "^0.7.5",
    "matrix-js-sdk": "^2.3.0",
    "tablecloth-bucklescript": "0.0.6"
  },
  "main": "index.js"
}
