  "scripts": {
    "bsb-clean": "bsb -clean-world",
    "bsb-build": "bsb -make-world",
    "bsb-watch": "bsb -make-world -w -ws _",
    "webpack-server": "webpack-dev-server",
    "webpack-server-tls": "webpack-dev-server --https --public imago-dev.img --key imago-dev.img.tls.key --cert imago-dev.img.tls.crt",
